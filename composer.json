{
    "name": "kejawenlab/semart-hris",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.7",
        "api-platform/core": "^2.1",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-fixtures-bundle": "^2.4",
        "doctrine/orm": "^2.4.5",
        "friendsofsymfony/jsrouting-bundle": "^2.0",
        "javiereguiluz/easyadmin-bundle": "^1.17",
        "kejawenlab/petruk-username": "^2.0",
        "league/csv": "^9.1",
        "nelmio/cors-bundle": "^1.5",
        "phpdocumentor/reflection-docblock": "^3.0 || ^4.0",
        "predis/predis": "^1.1",
        "snc/redis-bundle": "^2.0",
        "stof/doctrine-extensions-bundle": "^1.2",
        "symfony/asset": "^3.0 || ^4.0",
        "symfony/dotenv": "^4.0",
        "symfony/expression-language": "^3.0 || ^4.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/monolog-bundle": "^3.1",
        "symfony/proxy-manager-bridge": "^4.0",
        "symfony/security-bundle": "^3.0 || ^4.0",
        "symfony/stopwatch": "^3.3|^4.0",
        "symfony/twig-bundle": "^3.3|^4.0",
        "symfony/validator": "^3.0 || ^4.0",
        "symfony/web-profiler-bundle": "^3.3|^4.0",
        "symfony/web-server-bundle": "^4.0",
        "symfony/yaml": "^4.0",
        "vich/uploader-bundle": "^1.6"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.9.1",
        "phpunit/dbunit": "^3.0",
        "phpunit/php-invoker": "^1.1",
        "phpunit/phpunit": "^6.4",
        "symfony/flex": "^1.0",
        "symfony/phpunit-bridge": "^4.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "KejawenLab\\Application\\SemartHris\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\KejawenLab\\Application\\SemartHris\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "extra": {
        "symfony": {
            "id": "01BVZD4XBVC9W2SQTK5NK2173Q",
            "allow-contrib": true
        }
    }
}
